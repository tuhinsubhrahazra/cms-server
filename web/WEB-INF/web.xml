<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AddUserDetails</servlet-name>
        <servlet-class>cms_servlet.AddUserDetails</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateUser</servlet-name>
        <servlet-class>cms_servlet.CreateUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerifyUser</servlet-name>
        <servlet-class>cms_servlet.VerifyUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterComplain</servlet-name>
        <servlet-class>cms_servlet.RegisterComplain</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FetchAllComplain</servlet-name>
        <servlet-class>cms_servlet.FetchAllComplain</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateStatus</servlet-name>
        <servlet-class>cms_servlet.UpdateStatus</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckSession</servlet-name>
        <servlet-class>cms_servlet.CheckSession</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserInfo</servlet-name>
        <servlet-class>cms_servlet.UserInfo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddUserDetails</servlet-name>
        <url-pattern>/AddUserDetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateUser</servlet-name>
        <url-pattern>/CreateUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerifyUser</servlet-name>
        <url-pattern>/VerifyUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterComplain</servlet-name>
        <url-pattern>/RegisterComplain</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FetchAllComplain</servlet-name>
        <url-pattern>/FetchAllComplain</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateStatus</servlet-name>
        <url-pattern>/UpdateStatus</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserInfo</servlet-name>
        <url-pattern>/UserInfo</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
